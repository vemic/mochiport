{
  "name": "@ai-chat/backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",  "scripts": {
    "build": "tsc",
    "dev": "npm run build && func host start --port 7071",
    "dev:watch": "concurrently \"tsc --watch\" \"func host start --port 7071\"",
    "start": "func host start --port 7071",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\""
  },  "dependencies": {
    "@ai-chat/shared": "*",
    "@azure/functions": "^4.0.0",
    "@azure/cosmos": "^4.0.0",
    "zod": "^3.22.0",
    "uuid": "^9.0.0",
    "date-fns": "^3.0.0"
  },  "devDependencies": {
    "@ai-chat/tsconfig": "*",
    "@ai-chat/eslint-config": "*",
    "@types/uuid": "^9.0.0",
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "typescript": "^5.2.0",
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.0",
    "prettier": "^3.0.3",
    "eslint": "^8.57.0"
  }
}
