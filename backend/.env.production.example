# Production environment variables
# Copy this file to .env.production and fill in real values

AZURE_FUNCTIONS_ENVIRONMENT=Production
PORT=80
CORS_ORIGINS=https://your-frontend-domain.com
# Legacy PostgreSQL (deprecated - use Supabase instead)
DATABASE_CONNECTION_STRING=your-cosmos-db-connection-string
# Supabase Configuration
SUPABASE_URL=your-production-supabase-url
SUPABASE_ANON_KEY=your-production-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-production-supabase-service-role-key
REDIS_CONNECTION_STRING=your-redis-connection-string
JWT_SECRET=your-secure-jwt-secret-key-here
JWT_EXPIRATION=24h
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=60000
LOG_LEVEL=info
